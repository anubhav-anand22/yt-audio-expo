{
  "expo": {
    "name": "YTA",
    "slug": "yt-audio",
    "owner": "ak22git7488",
    "platforms": ["android"],
    "githubUrl": "https://github.com/anubhav-anand22/yt-audio-expo",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon1024.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/icon1024.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "versionCode": 1,
      "package": "com.herokuapp.ak22_audio",
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon1024.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/icon1024.png"
    }
  }
}
